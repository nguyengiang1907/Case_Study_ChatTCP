<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1000" prefWidth="1100" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.casestudy.ServerController">

    <children>
        <Label layoutX="190.0" layoutY="25.0" prefHeight="36.0" prefWidth="220.0" text="Chat Message">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <HBox />
        <TextField fx:id="textField" layoutX="44.0" layoutY="422.0" prefHeight="36.0" prefWidth="424.0" />

        <ScrollPane fx:id="spMain" layoutX="44.0" layoutY="61.0" prefHeight="347.0" prefWidth="525.0">
            <content>
                <VBox fx:id="hboxMessage" layoutX="63.0" layoutY="80.0" prefHeight="331.0" prefWidth="486.0">
                    <TextArea fx:id="textArea" prefHeight="331.0" prefWidth="540.0" />
                </VBox>
            </content>
        </ScrollPane>
         <Button layoutX="472.0" layoutY="422.0" mnemonicParsing="false" onAction="#send" prefHeight="36.0" prefWidth="52.0" text="Send" />
      <Button layoutX="537.0" layoutY="422.0" mnemonicParsing="false" onAction="#delete" prefHeight="36.0" prefWidth="52.0" text="Clear" />
    </children>
</AnchorPane>
